{
  "compilerOptions": {
    "target": "es5",
    "allowJs": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "preserve",
    "lib": ["es2015", "dom"],
    "rootDir": "src",
    "skipLibCheck": true,
    "types": [],
    "baseUrl": "src",
    "noImplicitAny": false
  },
  "include": ["src", "typings/*.d.ts"]
}
